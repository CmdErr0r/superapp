{
    "manifest_version":3,
    "version":"1.0.0",
    "name":"supertab",
    "permissions":["tabs", "activeTab", "newtab", "storage", "power", "bookmarks"],
    "background":{
        "scripts": ["background.js"]
    },
    "chrome_settings_overrides": {
        "homepage": "index.html"
    },
        "chrome_url_overrides": {
        "newtab": "index.html"
    }
}